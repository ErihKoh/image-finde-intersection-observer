{{#each this}}
<li class="photo-card">
    <img src="{{webformatURL}}" alt="{{tags}}" data-source="{{largeImageURL}}" width="360" height="240" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            <span>{{likes}}</span>
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</li>
{{/each}}